[Unit]
Description=Coinche Game Server
After=network.target

[Service]
Type=simple
User=coinche
WorkingDirectory=/home/coinche/Coinche/build
ExecStart=/home/coinche/Coinche/build/server
Restart=always
RestartSec=10

# Logs
StandardOutput=append:/var/log/coinche/server.log
StandardError=append:/var/log/coinche/error.log

[Install]
WantedBy=multi-user.target
